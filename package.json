{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "dev:staging": "vite --mode staging",
    "vtsc": "vue-tsc --noEmit --skipLibCheck",
    "build": "npm run vtsc && vite build",
    "build:dev": "npm run vtsc && vite build --mode development",
    "build:staging": "npm run vtsc && vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .vue,.js,jsx,.ts,tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,jsx,.ts,tsx"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.3",
    "naive-ui": "^2.27.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.23",
    "@iconify/vue": "^3.2.1",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "lint-staged": "^12.3.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.1",
    "rollup": "^2.70.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.18.5",
    "vite": "*",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-windicss": "^1.8.3",
    "vue-tsc": "^0.33.9",
    "vueuc": "^0.4.28",
    "windicss": "^3.5.1"
  }
}