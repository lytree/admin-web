{
	"name": "admin-web",
	"version": "1.0.0",
	"scripts": {
		"dev": "cross-env VITE_ENV_TYPE=dev vite",
		"dev:test": "cross-env VITE_ENV_TYPE=test vite",
		"dev:prod": "cross-env VITE_ENV_TYPE=prod vite",
		"build": "npm run typecheck && cross-env VITE_ENV_TYPE=prod vite build",
		"build:dev": "npm run typecheck && cross-env VITE_ENV_TYPE=dev vite build",
		"build:test": "npm run typecheck && cross-env VITE_ENV_TYPE=test vite build",
		"build:vercel": "cross-env VITE_HASH_ROUTE=true vite build",
		"preview": "vite preview",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
		"prepare": "husky install",
		"postinstall": "patch-package",
		"release": "standard-version",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
	},
	"lint-staged": {
		"*.{vue,js,jsx,ts,tsx}": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	},
	"dependencies": {
		"@antv/data-set": "^0.11.8",
		"@antv/g2": "^4.2.7",
		"@better-scroll/core": "^2.4.2",
		"@vueuse/core": "^9.2.0",
		"axios": "^0.27.2",
		"clipboard": "^2.0.11",
		"colord": "^2.9.3",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.5",
		"echarts": "^5.3.3",
		"form-data": "^4.0.0",
		"lodash-es": "^4.17.21",
		"naive-ui": "^2.33.2",
		"pinia": "^2.0.22",
		"print-js": "^1.6.0",
		"qs": "^6.11.0",
		"swiper": "^8.3.2",
		"ua-parser-js": "^1.0.2",
		"vditor": "^3.8.17",
		"vue": "^3.2.39",
		"vue-router": "^4.1.5",
		"wangeditor": "^4.7.15",
		"xgplayer": "^2.32.0"
	},
	"devDependencies": {
		"@amap/amap-jsapi-types": "^0.0.8",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@iconify/json": "^2.1.104",
		"@iconify/vue": "^3.2.1",
		"@types/bmapgl": "^0.0.5",
		"@types/crypto-js": "^4.1.1",
		"@types/node": "^17.0.45",
		"@types/qs": "^6.9.7",
		"@types/ua-parser-js": "^0.7.36",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"@vitejs/plugin-vue": "^3.1.0",
		"@vitejs/plugin-vue-jsx": "^2.0.1",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^10.0.0",
		"commitizen": "^4.2.5",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^6.9.2",
		"eslint": "^8.23.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^8.7.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"mockjs": "^1.1.0",
		"patch-package": "^6.4.7",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.7.1",
		"rollup-plugin-visualizer": "^5.8.1",
		"sass": "^1.54.9",
		"standard-version": "^9.5.0",
		"typescript": "^4.8.3",
		"unocss": "^0.44.7",
		"unplugin-auto-import": "^0.11.2",
		"unplugin-icons": "^0.14.9",
		"unplugin-vue-components": "0.22.4",
		"vite": "^3.1.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-mock": "^2.9.6",
		"vue-eslint-parser": "^9.0.3",
		"vue-tsc": "^0.40.13"
	}
}
